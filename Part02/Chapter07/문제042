#include <iostream>

using namespace std;

int main()
{
  ios::sync_with_stdio(false);
  cin.tie(NULL);
  cout.tie(NULL);

  int T, A, B;
  cin >> T;
  for(int i = 0; i < T; i++)
  {
    cin >> A >> B;

    int j = 1;
    while(true)
    {
      if(B * j % A == 0)
      {
        cout << B * j << endl;
        break;
      }
      j++;
    }
  }
}
